{% extends 'base.html' %}

{% block content %}
  <h1>Adicionar usuário ao projeto "{{ project.name }}"</h1>
  <form method="post">
    {% csrf_token %}
    <label for="user">Usuário:</label>
    <select name="user">
      {% for user in users %}
        <option value="{{ user.id }}">{{ user.name }}</option>
      {% endfor %}
    </select><br><br>
    <button type="submit">Adicionar usuário</button>
  </form>
{% endblock %}
